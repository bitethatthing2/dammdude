{
  "name": "side-hustle-bar-pwa",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=v22.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:https": "node dev-https.js",
    "build": "next build",
    "postbuild": "node scripts/populate-service-worker.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --max-warnings=0",
    "lint:report": "next lint --output-file eslint-report.json --format json",
    "fix:all": "npm run fix:eslint && npm run fix:prettier && npm run fix:imports",
    "fix:eslint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "fix:eslint:errors": "eslint . --fix --quiet --ext .js,.jsx,.ts,.tsx",
    "fix:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "fix:imports": "node scripts/fix-imports.js",
    "fix:unused": "eslint . --fix --rule '{\"@typescript-eslint/no-unused-vars\": \"error\"}'",
    "fix:entities": "node scripts/fix-unescaped-entities.js",
    "fix:any": "eslint . --fix --rule '{\"@typescript-eslint/no-explicit-any\": \"error\"}'",
    "fix:supabase": "node scripts/fix-supabase-clients.js",
    "clean": "rimraf .next && rimraf node_modules/.cache",
    "clean:lint": "rimraf eslint-report.json",
    "migrate": "powershell -ExecutionPolicy Bypass -File ./migrate_orders.ps1",
    "check-imports": "node scripts/check-imports.js",
    "verify-imports": "node verify-imports.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "debug:deprecation": "node --trace-deprecation node_modules/.bin/next dev",
    "gen-types": "supabase gen types typescript --project-id tvnpgbjypnezoasbhbwx > lib/database.types.ts",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "build:production": "cross-env NODE_ENV=production next build",
    "lighthouse": "lhci autorun",
    "lighthouse:view": "lhci open",
    "performance": "npm run build && npm run lighthouse",
    "generate-icons": "node scripts/generate-icons.js",
    "optimize-images": "node scripts/optimize-images.js",
    "precache-manifest": "workbox generateSW workbox-config.js",
    "test:pwa": "npm run build && npx serve@latest out",
    "cache:clear": "rimraf .next/cache && rimraf node_modules/.cache",
    "prefetch:images": "node scripts/prefetch-menu-images.js"
  },
